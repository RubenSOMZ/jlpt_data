{
  "grammar": [
    { "id": "n5-1", "category": "Partícula", "title": "は (wa)", "formula": "S + は", "description": "Marcador de tema. Indica de qué estamos hablando.", "examples": [{ "jp": "私[わたし]は 学生[がくせい]です。", "es": "Yo soy estudiante." }, { "jp": "これ[これ]は ペンです。", "es": "Esto es un bolígrafo." }] },
    { "id": "n5-2", "category": "Partícula", "title": "が (ga)", "formula": "S + が", "description": "Marcador de sujeto. Destaca quién realiza la acción.", "examples": [{ "jp": "猫[ねこ]が います。", "es": "Hay un gato." }, { "jp": "犬[いぬ]が 走[はし]ります。", "es": "Un perro corre." }] },
    { "id": "n5-3", "category": "Partícula", "title": "を (wo)", "formula": "S + を", "description": "Marcador de objeto directo. Indica qué recibe la acción del verbo.", "examples": [{ "jp": "りんごを 食[た]べます。", "es": "Como una manzana." }, { "jp": "水[みず]を 飲[の]みます。", "es": "Bebo agua." }] },
    { "id": "n5-4", "category": "Partícula", "title": "に (ni)", "formula": "Lugar/Tiempo + に", "description": "Indica destino, punto en el tiempo o lugar de existencia.", "examples": [{ "jp": "学校[がっこう]に 行[い]きます。", "es": "Voy a la escuela." }] },
    { "id": "n5-5", "category": "Partícula", "title": "で (de)", "formula": "Lugar + で", "description": "Indica el lugar donde ocurre una acción o el medio utilizado.", "examples": [{ "jp": "図書館[としょかん]で 勉強[べんきょう]します。", "es": "Estudio en la biblioteca." }] },
    { "id": "n5-6", "category": "Partícula", "title": "へ (e)", "formula": "Lugar + へ", "description": "Indica la dirección de un movimiento.", "examples": [{ "jp": "日本[にほん]へ 行[い]きます。", "es": "Voy hacia Japón." }] },
    { "id": "n5-7", "category": "Partícula", "title": "の (no)", "formula": "S1 + の + S2", "description": "Indica posesión o relación entre sustantivos.", "examples": [{ "jp": "私[わたし]の 本[ほん]。", "es": "Mi libro." }] },
    { "id": "n5-8", "category": "Partícula", "title": "と (to)", "formula": "S + と", "description": "Significa 'y' (conectar sustantivos) o 'con' (compañía).", "examples": [{ "jp": "友達[ともだち]と 映画[えいが]を 見[み]ます。", "es": "Veo una película con un amigo." }] },
    { "id": "n5-9", "category": "Partícula", "title": "か (ka)", "formula": "Frase + か", "description": "Marcador de pregunta al final de la oración.", "examples": [{ "jp": "学生[がくせい]ですか。", "es": "¿Eres estudiante?" }] },
    { "id": "n5-10", "category": "Partícula", "title": "も (mo)", "formula": "S + も", "description": "Significa 'también' o 'tampoco'.", "examples": [{ "jp": "私[わたし]も 学生[がくせい]です。", "es": "Yo también soy estudiante." }] },
    { "id": "n5-11", "category": "Patrón", "title": "です / だ", "formula": "S + です", "description": "Cópula 'ser' o 'estar' en forma cortés e informal.", "examples": [{ "jp": "これは 本[ほん]です。", "es": "Esto es un libro." }] },
    { "id": "n5-12", "category": "Patrón", "title": "じゃない / ではない", "formula": "S + じゃない", "description": "Forma negativa de 'ser' (no ser).", "examples": [{ "jp": "学生[がくせい]じゃないです。", "es": "No soy estudiante." }] },
    { "id": "n5-13", "category": "Patrón", "title": "でした", "formula": "S + でした", "description": "Pasado de 'ser' (era o fue).", "examples": [{ "jp": "昨日[きのう]は 雨[あめ]でした。", "es": "Ayer fue un día de lluvia." }] },
    { "id": "n5-14", "category": "Patrón", "title": "じゃなかった", "formula": "S + じゃなかった", "description": "Pasado negativo de 'ser' (no era o no fue).", "examples": [{ "jp": "学生[がくせい]じゃなかったです。", "es": "No era estudiante." }] },
    { "id": "n5-15", "category": "Verbo", "title": "～ます (masu)", "formula": "Raíz V + ます", "description": "Forma cortés para presente y futuro.", "examples": [{ "jp": "食[た]べます。", "es": "Como / Comeré." }] },
    { "id": "n5-16", "category": "Verbo", "title": "～ません (masen)", "formula": "Raíz V + ません", "description": "Forma negativa cortés (no comer/ir).", "examples": [{ "jp": "食[た]べません。", "es": "No como / No comeré." }] },
    { "id": "n5-17", "category": "Verbo", "title": "～ました (mashita)", "formula": "Raíz V + ました", "description": "Forma pasada cortés.", "examples": [{ "jp": "食[た]べました。", "es": "Comí." }] },
    { "id": "n5-18", "category": "Verbo", "title": "～ませんでした", "formula": "Raíz V + ませんでした", "description": "Forma pasada negativa cortés.", "examples": [{ "jp": "食[た]べませんでした。", "es": "No comí." }] },
    { "id": "n5-19", "category": "Verbo", "title": "～ましょう", "formula": "Raíz V + ましょう", "description": "Invitación u ofrecimiento: 'vamos a' o 'hagamos'.", "examples": [{ "jp": "食[た]べましょう。", "es": "Comamos." }] },
    { "id": "n5-20", "category": "Verbo", "title": "～ましょうか", "formula": "Raíz V + ましょうか", "description": "¿Quieres que...? o ¿Hacemos...?", "examples": [{ "jp": "食[た]べましょうか。", "es": "¿Comemos? (¿te parece bien?)" }] },
    { "id": "n5-21", "category": "Verbo", "title": "～ませんか", "formula": "Raíz V + ませんか", "description": "Invitación educada: '¿por qué no...?'", "examples": [{ "jp": "食[た]べませんか。", "es": "¿No quieres comer?" }] },
    { "id": "n5-22", "category": "Petición", "title": "～てください", "formula": "V[te] + ください", "description": "Por favor, haz algo (petición).", "examples": [{ "jp": "見[み]てください。", "es": "Por favor, mira." }] },
    { "id": "n5-23", "category": "Verbo", "title": "～ている", "formula": "V[te] + いる", "description": "Acción en progreso o estado continuo.", "examples": [{ "jp": "食[た]べています。", "es": "Estoy comiendo." }] },
    { "id": "n5-24", "category": "Tiempo", "title": "～てから", "formula": "V[te] + から", "description": "Después de hacer algo.", "examples": [{ "jp": "食[た]べてから 行[い]きます。", "es": "Iré después de comer." }] },
    { "id": "n5-25", "category": "Concesivo", "title": "～ても / ～でも", "formula": "V[te] + も", "description": "Incluso si... o aunque...", "examples": [{ "jp": "雨[あめ]でも 行[い]きます。", "es": "Iré incluso si llueve." }] },
    { "id": "n5-26", "category": "Adjetivo", "title": "Adj-i Presente", "formula": "Adj-i + です", "description": "Adjetivo tipo 'i' en presente.", "examples": [{ "jp": "大[おお]きいです。", "es": "Es grande." }] },
    { "id": "n5-27", "category": "Adjetivo", "title": "Adj-i Negativo", "formula": "Adj-i (-i) + くない", "description": "Adjetivo tipo 'i' en negativo.", "examples": [{ "jp": "大[おお]きくないです。", "es": "No es grande." }] },
    { "id": "n5-28", "category": "Adjetivo", "title": "Adj-i Pasado", "formula": "Adj-i (-i) + かった", "description": "Adjetivo tipo 'i' en pasado.", "examples": [{ "jp": "大[おお]きかったです。", "es": "Era grande / fue grande." }] },
    { "id": "n5-29", "category": "Adjetivo", "title": "Adj-i Pasado Neg.", "formula": "Adj-i (-i) + くなかった", "description": "Adjetivo tipo 'i' en pasado negativo.", "examples": [{ "jp": "大[おお]きくなかったです。", "es": "No era grande / no fue grande." }] },
    { "id": "n5-30", "category": "Adjetivo", "title": "Adj-na Presente", "formula": "Adj-na + です", "description": "Adjetivo tipo 'na' en presente.", "examples": [{ "jp": "静[しず]かです。", "es": "Es silencioso / está tranquilo." }] },
    { "id": "n5-31", "category": "Adjetivo", "title": "Adj-na Negativo", "formula": "Adj-na + じゃない", "description": "Adjetivo tipo 'na' en negativo.", "examples": [{ "jp": "静[しず]かじゃないです。", "es": "No es silencioso / no está tranquilo." }] },
    { "id": "n5-32", "category": "Existencia", "title": "います", "formula": "S + が います", "description": "Existencia de personas o animales (seres animados).", "examples": [{ "jp": "犬[いぬ]が います。", "es": "Hay un perro." }] },
    { "id": "n5-33", "category": "Existencia", "title": "あります", "formula": "S + が あります", "description": "Existencia de objetos o plantas (seres inanimados).", "examples": [{ "jp": "本[ほん]が あります。", "es": "Hay un libro." }] },
    { "id": "n5-34", "category": "Deseo", "title": "～たい", "formula": "Raíz V + たい", "description": "Querer hacer algo (primera persona).", "examples": [{ "jp": "食[た]べたいです。", "es": "Quiero comer." }] },
    { "id": "n5-35", "category": "Deseo", "title": "～たくない", "formula": "Raíz V + たくない", "description": "No querer hacer algo.", "examples": [{ "jp": "食[た]べたくないです。", "es": "No quiero comer." }] },
    { "id": "n5-36", "category": "Deseo", "title": "～が欲[ほ]しい", "formula": "S + が ほしい", "description": "Querer un objeto.", "examples": [{ "jp": "車[くるま]が 欲[ほ]しいです。", "es": "Quiero un coche." }] },
    { "id": "n5-37", "category": "Capacidad", "title": "できる", "formula": "V[dic] + こと が できる", "description": "Poder o ser capaz de hacer algo.", "examples": [{ "jp": "日本[にほん]語[ご]が できます。", "es": "Puedo hablar japonés (Sé japonés)." }] },
    { "id": "n5-38", "category": "Gusto", "title": "～が好[す]き", "formula": "S + が 好き", "description": "Gustar algo.", "examples": [{ "jp": "寿司[すし]が 好[す]きです。", "es": "Me gusta el sushi." }] },
    { "id": "n5-39", "category": "Gusto", "title": "～が嫌[きら]い", "formula": "S + が 嫌い", "description": "Disgustar o odiar algo.", "examples": [{ "jp": "野菜[やさい]が 嫌[きら]いです。", "es": "No me gustan las verduras." }] },
    { "id": "n5-40", "category": "Habilidad", "title": "～が上手[じょうず]", "formula": "S + が 上手", "description": "Ser bueno en algo.", "examples": [{ "jp": "料理[りょうり]が 上手[じょうず]です。", "es": "Eres bueno cocinando." }] },
    { "id": "n5-41", "category": "Habilidad", "title": "～が下手[へた]", "formula": "S + が 下手", "description": "Ser malo en algo.", "examples": [{ "jp": "歌[うた]が 下手[へた]です。", "es": "Soy malo cantando." }] },
    { "id": "n5-42", "category": "Comprensión", "title": "～が分[わ]かる", "formula": "S + が わかる", "description": "Entender o comprender algo.", "examples": [{ "jp": "日本[にほん]語[ご]が 分[わ]かります。", "es": "Entiendo el japonés." }] },
    { "id": "n5-43", "category": "Necesidad", "title": "～が要[い]る", "formula": "S + が いる", "description": "Necesitar algo.", "examples": [{ "jp": "お金[かね]が 要[い]ります。", "es": "Necesito dinero." }] },
    { "id": "n5-44", "category": "Cantidad", "title": "～くらい / ぐらい", "formula": "Cant. + ぐらい", "description": "Aproximadamente (cantidad o duración).", "examples": [{ "jp": "三[さん]時間[じかん]ぐらい。", "es": "Unas tres horas." }] },
    { "id": "n5-45", "category": "Tiempo", "title": "～ごろ", "formula": "Tiempo + ごろ", "description": "Alrededor de (punto específico en el tiempo).", "examples": [{ "jp": "三[さん]時[じ]ごろ。", "es": "Alrededor de las tres." }] },
    { "id": "n5-46", "category": "Limitación", "title": "～しか～ない", "formula": "S + しか + V[neg]", "description": "Solo / Solamente (con matiz de que es poco).", "examples": [{ "jp": "一つ[ひとつ]しか ない。", "es": "Solo hay uno." }] },
    { "id": "n5-47", "category": "Conjunto", "title": "そして", "formula": "F1. そして F2", "description": "Y / Además (conecta oraciones).", "examples": [{ "jp": "勉強[べんきょう]します。そして、寝[ね]ます。", "es": "Estudio. Y después, duermo." }] },
    { "id": "n5-48", "category": "Conjunto", "title": "それから", "formula": "F1. それから F2", "description": "Después de eso / Y entonces.", "examples": [{ "jp": "食[た]べます。それから、行[い]きます。", "es": "Como. Después, iré." }] },
    { "id": "n5-49", "category": "Conjunto", "title": "でも", "formula": "F1. でも F2", "description": "Pero / Sin embargo.", "examples": [{ "jp": "高[たか]いです。でも、買[か]います。", "es": "Es caro. Pero lo compraré." }] },
    { "id": "n5-50", "category": "Conjunto", "title": "だから", "formula": "F1. だから F2", "description": "Así que / Por lo tanto.", "examples": [{ "jp": "暑[あつ]いです。だから、窓[まど]を 開[あ]けます。", "es": "Hace calor. Por eso, abriré la ventana." }] },
    { "id": "n5-51", "category": "Interrogativo", "title": "何 (nani/nan)", "formula": "Interrogativo", "description": "¿Qué?", "examples": [{ "jp": "これは 何[なん]ですか。", "es": "¿Qué es esto?" }] },
    { "id": "n5-52", "category": "Interrogativo", "title": "誰 (dare)", "formula": "Interrogativo", "description": "¿Quién?", "examples": [{ "jp": "あれは 誰[だれ]ですか。", "es": "¿Quién es aquel?" }] },
    { "id": "n5-53", "category": "Interrogativo", "title": "どこ (doko)", "formula": "Interrogativo", "description": "¿Dónde?", "examples": [{ "jp": "どこに 行[い]きますか。", "es": "¿A dónde vas?" }] },
    { "id": "n5-54", "category": "Interrogativo", "title": "いつ (itsu)", "formula": "Interrogativo", "description": "¿Cuándo?", "examples": [{ "jp": "いつ 来[き]ますか。", "es": "¿Cuándo vendrás?" }] },
    { "id": "n5-55", "category": "Interrogativo", "title": "どう (dou)", "formula": "Interrogativo", "description": "¿Cómo?", "examples": [{ "jp": "どうですか。", "es": "¿Cómo está? / ¿Qué tal?" }] },
    { "id": "n5-56", "category": "Interrogativo", "title": "どうして", "formula": "Interrogativo", "description": "¿Por qué?", "examples": [{ "jp": "どうして 行[い]きますか。", "es": "¿Por qué vas?" }] },
    { "id": "n5-57", "category": "Interrogativo", "title": "いくら (ikura)", "formula": "Interrogativo", "description": "¿Cuánto cuesta?", "examples": [{ "jp": "いくらですか。", "es": "¿Cuánto cuesta?" }] },
    { "id": "n5-58", "category": "Interrogativo", "title": "どれ (dore)", "formula": "Interrogativo", "description": "¿Cuál? (entre varios objetos).", "examples": [{ "jp": "どれが 好[す]きですか。", "es": "¿Cuál te gusta?" }] },
    { "id": "n5-59", "category": "Interrogativo", "title": "どの (dono)", "formula": "どの + S", "description": "¿Qué...?", "examples": [{ "jp": "どの 本[ほん]ですか。", "es": "¿Qué libro es?" }] },
    { "id": "n5-60", "category": "Comparación", "title": "～より", "formula": "A は B より", "description": "Comparación: 'A es más ... que B'.", "examples": [{ "jp": "これは あれより 大[おお]きい。", "es": "Esto es más grande que aquello." }] },
    { "id": "n5-61", "category": "Comparación", "title": "～の方[ほう]が", "formula": "A のほうが B より", "description": "Indica la opción preferida o superior.", "examples": [{ "jp": "こちらの 方[ほう]が 好[す]きです。", "es": "Me gusta más este." }] },
    { "id": "n5-62", "category": "Comparación", "title": "一番[いちばん]", "formula": "一番 + Adj", "description": "El más... (superlativo).", "examples": [{ "jp": "これが 一番[いちばん] 好[す]きです。", "es": "Este es el que más me gusta." }] },
    { "id": "n5-63", "category": "Tiempo", "title": "～前[まえ]に", "formula": "V[dic] / S + の + 前に", "description": "Antes de algo.", "examples": [{ "jp": "寝[ね]る 前[まえ]に 歯[は]を 磨[みが]きます。", "es": "Me lavo los dientes antes de dormir." }] },
    { "id": "n5-64", "category": "Tiempo", "title": "～後[あと]で", "formula": "V[ta] / S + の + あとで", "description": "Después de algo.", "examples": [{ "jp": "食[た]べた 後[あと]で 休[やす]みます。", "es": "Descansaré después de comer." }] },
    { "id": "n5-65", "category": "Tiempo", "title": "～から", "formula": "Tiempo + から", "description": "Desde un punto temporal.", "examples": [{ "jp": "九[く]時[じ]から 働[はたら]きます。", "es": "Trabajo desde las 9." }] },
    { "id": "n5-66", "category": "Tiempo", "title": "～まで", "formula": "Tiempo + まで", "description": "Hasta un punto temporal.", "examples": [{ "jp": "五[ご]時[じ]まで 働[はたら]きます。", "es": "Trabajo hasta las 5." }] },
    { "id": "n5-67", "category": "Tiempo", "title": "～間[あいだ]", "formula": "S + の + あいだ", "description": "Durante / Mientras.", "examples": [{ "jp": "休[やす]みの 間[あいだ] 旅行[りょこう]します。", "es": "Viajaré durante las vacaciones." }] },
    { "id": "n5-68", "category": "Frecuencia", "title": "いつも", "formula": "Adverbio", "description": "Siempre.", "examples": [{ "jp": "いつも 勉強[べんきょう]します。", "es": "Siempre estudio." }] },
    { "id": "n5-69", "category": "Frecuencia", "title": "よく", "formula": "Adverbio", "description": "Frecuentemente / A menudo.", "examples": [{ "jp": "よく 映画[えいが]を 見[み]ます。", "es": "Veo películas a menudo." }] },
    { "id": "n5-70", "category": "Frecuencia", "title": "時々[ときどき]", "formula": "Adverbio", "description": "A veces.", "examples": [{ "jp": "時々[ときどき] 料理[りょうり]します。", "es": "A veces cocino." }] },
    { "id": "n5-71", "category": "Frecuencia", "title": "あまり～ない", "formula": "あまり + V[neg]", "description": "No mucho / No muy frecuentemente.", "examples": [{ "jp": "あまり 食[た]べません。", "es": "No como mucho." }] },
    { "id": "n5-72", "category": "Frecuencia", "title": "全然[ぜんぜん]～ない", "formula": "全然 + V[neg]", "description": "Para nada / Nunca.", "examples": [{ "jp": "全然[ぜんぜん] 分[わ]かりません。", "es": "No entiendo nada en absoluto." }] }
  ]
}
